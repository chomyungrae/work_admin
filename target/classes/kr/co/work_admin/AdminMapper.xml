<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybats-3-mapper.dtd">
  <mapper namespace="kr.co.work_admin.AdminMapper">
   
   <insert id="work_input_ok">
   insert into work(workday,workname,workplace,content,start_time,close_time,inwon,gender,pay,writeday)
   values(#{workday},#{workname},#{workplace},#{content},#{start_time},#{close_time},
   #{inwon},#{gender},#{pay},now())
   </insert>
  
  </mapper>